{
    "manifest_version": 3,
    "name": "Search SimeonOnSecurity",
    "version": "1.0",
    "description": "Search SimeonOnSecurity directly from the browser.",
    "permissions": "",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/16.png",
            "48": "images/48.png",
            "128": "images/128.png"
        }
    },
    "icons": {
        "16": "images/16.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "search@simeononsecurity.ch"
        },
        "gecko_android": {
            "id": "search@simeononsecurity.ch"
        }
    }
}